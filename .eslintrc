{
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "parserOptions": {
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "camelcase": 0,
    "consistent-return": 0,
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "linebreak-style": [0],
    "no-console": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-undef": 0,
    "no-unneeded-ternary": 0,
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "no-var": 0,
    "prefer-const": 0,
    "spaced-comment": 0,
    "arrow-body-style": [0, "as-needed"],
    "jsx-control-statements/jsx-use-if-tag": 0,
    "no-restricted-syntax": [1, "DebuggerStatement"] // 1-warn
  }
}
